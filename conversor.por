programa {
  funcao inicio() {

    real moeda
    inteiro opcao

    escreva("Olá! Escolha para qual moeda deseja converter: ")
    escreva("1 - Iene (JPY)")
    escreva("2 - Dolar Canadense (CAD)")
    escreva("3 - Peso chileno (CLP)")
    leia(opcao)

    conversor(opcao)
  }

  funcao conversor(){
    escolha (opcao){
      caso 1:
      pare

      caso 2:
      pare

      caso 3:
      pare
    }

  }
}
